{
  "name": "rumur/autowire",
  "description": "Simple PHP autowiring functionality.",
  "keywords": ["wordpress", "plugin", "autowire", "autowiring"],
  "homepage": "https://github.com/rumur/autowire",
  "license": "MIT",
  "authors": [
    {
      "name": "rumur",
      "email": "rumur.dev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0"
  },
  "autoload" : {
    "psr-4" : {
      "Rumur\\Autowiring\\": "src/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -v",
    "sniff": "./vendor/bin/phpcs -v",
    "sniff-fix": "./vendor/bin/phpcbf -v"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "phpunit/phpunit": "^8.5"
  }
}